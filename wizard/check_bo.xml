<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <!-- check.order form view -->
    <record id="check_bo_wizard_view_form" model="ir.ui.view">
        <field name="name">Message Booking Order</field>
        <field name="model">check.bo.wizard</field>
        <field name="arch" type="xml">
            <form string="Chek Message">
                <sheet>
                      <field name="message" attrs="{'readonly': True}"/> 
                     <footer>
                        <button string="Close" class="btn-secondary" special="cancel" />
                    </footer>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
